LABEL raid
#MENU HIDE
KERNEL 64bit/current/vmlinuz
APPEND initrd=64bit/current/initrd.img ksdevice=bootif ks=http://www.salstar.sk/ks/auto part=root=4,vg=test2,swap=1,var=2

LABEL kvm
#MENU HIDE
KERNEL 64bit/current/vmlinuz
APPEND nodhcp initrd=64bit/current/initrd.img ksdevice=bootif ks=http://www.salstar.sk/ks/auto part=root=8,swap=8,vg=kvm

LABEL 4g
